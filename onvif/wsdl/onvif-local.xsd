						<xs:element name="Type" type="tt:ClassType"/>
						<xs:element name="Likelihood" type="xs:float"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Extension" type="tt:ClassDescriptorExtension" minOccurs="0"/>
